# Docker Compose Environment Variables
# Copy this to .env and fill in your values

# Database
POSTGRES_DB=wekeza_banking
POSTGRES_USER=wekeza_user
POSTGRES_PASSWORD=change_this_password

# API Server
NODE_ENV=development
PORT=3000

# JWT Secrets - CHANGE THESE IN PRODUCTION
JWT_SECRET=change_this_jwt_secret_to_something_secure
JWT_REFRESH_SECRET=change_this_refresh_secret_to_something_secure

# OAuth
OAUTH_CLIENT_ID=sandbox_client
OAUTH_CLIENT_SECRET=change_this_oauth_secret

# Webhook
WEBHOOK_SECRET=change_this_webhook_secret
